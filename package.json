{
  "name": "a-nice-bot",
  "version": "0.0.1",
  "description": "Say hello, to A Nice Bot. Created to learn Node.js and the Discord API, this bot is a general purpose bot to play around with and have fun, while at the same time providing many features such as server management.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node .",
    "lint": "eslint .",
    "auto-start": "nodemon -e . -x \"npm run start\"",
    "web": "cd web && node server.js",
    "auto-web": "nodemon -e ./web/server.js -x \"npm run web\"",
    "sass": "node node_modules/node-sass/bin/node-sass ./web/style.scss ./web/style.css && node ./web/minify-css.js ./web/style.css",
    "auto-sass": "nodemon -e *.scss -x \"npm run sass\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-Noah/A-Nice-Bot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "discord",
    "bot"
  ],
  "author": "The Noah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/The-Noah/A-Nice-Bot/issues"
  },
  "website": "http://anicebot.freemyip.com:8080/",
  "homepage": "https://github.com/The-Noah/A-Nice-Bot#readme",
  "dependencies": {
    "discord.js": "^11.4.2",
    "form-data": "^2.3.3",
    "markdown": "^0.5.0",
    "node-fetch": "^2.3.0",
    "node-json-db": "^0.9.1",
    "node-persist": "^2.1.0"
  },
  "devDependencies": {
    "clean-css": "^4.2.1",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6"
  }
}
